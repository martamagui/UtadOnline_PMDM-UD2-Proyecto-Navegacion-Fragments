<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomTitle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">42sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="CustomSecondTitle" parent="CustomTitle">
        <item name="android:textColor">@color/secondTitleColor</item>
    </style>
    <style name="MyCustomButton">
        <item name="android:backgroundTint">@color/myColor</item>
        <item name="strokeColor">@color/black</item>
        <item name="strokeWidth">2dp</item>
    </style>
</resources>